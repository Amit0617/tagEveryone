{

  "manifest_version": 2,
  "name": "@everyone",
  "version": "1.3.5",
  "description": "Tags Everyone in a WhatsApp group",
  "icons": {
    "48": "icons/everyone-48.png",
    "96": "icons/everyone-96.png"
  },
  
  "permissions": [
    "activeTab"
  ],
  
  "browser_action": {
    "default_icon": "icons/everyone-32.png",
    "default_title": "tagEveryone!"
  },  

  "background": {
    "scripts": ["background.js"]
  }

}
